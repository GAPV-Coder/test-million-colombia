{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "MillionApi",
    "Audience": "MillionClient",
    "Secret": "${JWT_SECRET_KEY}",
    "ExpiresInMinutes": 60
  },
  "MongoSettings": {
    "ConnectionString": "${MONGODB_URI}",
    "DatabaseName": "MillionDb"
  },
  "AllowedHosts": "*"
}
